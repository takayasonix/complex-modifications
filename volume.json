{
  "title": "volume // fnを押しながらb/nを押すと、音量を減らす/増やすを送信する",
  "rules": [
    {
      "description": "volume_decrement // [fn]+[b]=[volume_decrement]",
      "manipulators": [
        {
          "from": {
            "key_code": "b",
            "modifiers": { "mandatory": [ "fn" ] }
          },
          "to": [
            {"key_code": "volume_decrement"}
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "volume_increment // [fn]+[c]=[volume_increment]",
      "manipulators": [
        {
          "from": {
            "key_code": "n",
            "modifiers": { "mandatory": [ "fn" ] }
          },
          "to": [
            {"key_code": "volume_increment"}
          ],
          "type": "basic"
        }
      ]
    }
  ]
}